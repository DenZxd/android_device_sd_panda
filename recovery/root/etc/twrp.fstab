# mount point  fstype  device                                                  flags

/sdcard        vfat    /dev/block/mmcblk1p1 /dev/block/mmcblk1 0
/data          ext4    /dev/block/mmcblk0p9
/cache         ext4    /dev/block/mmcblk0p8
/system        ext4    /dev/block/mmcblk0p7
/boot          emmc    /dev/block/mmcblk0p5
/recovery      emmc    /dev/block/mmcblk0p4
